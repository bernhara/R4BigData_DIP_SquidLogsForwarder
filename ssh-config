Host log-collector-*
   User dip
   IdentityFile ~/R4BigData_DIP_SquidLogsForwarder/ssh-key-to-s-proxetnet
   StrictHostKeyChecking no
   UserKnownHostsFile /dev/null
   ConnectTimeout 5

Host log-collector-lan
   HostName s-proxetnet.home
   Port 22

Host log-collector-wan
   HostName s-proxetnet.home
   Port 22
   ProxyCommand ssh -F ${HOME}/R4BigData_DIP_SquidLogsForwarder/ssh-config log-collector-public-sshd-server "nc %h %p"

Host ssh log-collector-public-sshd-server
   HostName s-m2mgw.sytes.net
   Port 443
   IdentityFile ~/R4BigData_DIP_SquidLogsForwarder/ssh-key-to-s-proxetnet